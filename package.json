  "scripts": {
    "start": "npm run tiles && concurrently \"react-scripts start\" \"npm run generate-watch\"",
    "prebuild": "npm run i18n && npm run tiles && npm run generate",
    "build": "react-scripts build",
    "i18n": "node scripts/i18n.js",
    "tiles": "node scripts/tilespecs.js",
    "generate": "dotenv -- graphql-codegen --config scripts/codegen.yml",
    "generate-watch": "dotenv -- graphql-codegen --config scripts/codegen.yml --watch",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.1",
    "@graphql-codegen/fragment-matcher": "^3.1.0",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/typescript": "^2.2.4",
    "@graphql-codegen/typescript-apollo-client-helpers": "^2.1.6",
    "@graphql-codegen/typescript-document-nodes": "^2.1.6",
    "@graphql-codegen/typescript-graphql-files-modules": "^2.1.0",
    "@graphql-codegen/typescript-operations": "^2.1.8",
    "@graphql-codegen/typescript-react-apollo": "^3.1.6",
    "@graphql-codegen/urql-introspection": "^2.1.0",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/node-logger": "^6.3.12",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.12",
    "@types/d3-format": "^3.0.1",
    "@types/jest": "^26.0.22",
    "@types/mapbox-gl": "^2.1.2",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-swipeable-views": "^0.13.0",
    "@types/react-swipeable-views-utils": "^0.13.2",
    "@types/styled-components": "^5.1.14",
    "@types/uuid": "^8.3.1",
    "apollo": "^2.33.2",
    "concurrently": "^6.1.0",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "iv": "^0.0.1",
    "node-fetch": "^2.6.1",
    "storybook-addon-styled-components-themes": "^0.0.2",
